{
  "name": "ecalculareactfluxssrcoreapp",
  "version": "1.0.0",
  "description": "Ecalcula Core App",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:server": "babel ./app/server -d build/server",
    "build:watch:server": "babel ./app/server -d build/server --watch",
    "build:client": "webpack --mode production --config ./webpack.prod.config.js/",
    "build:watch:client": "webpack --mode development --config ./webpack.config.js/ --watch",
    "start:dev": "parallelshell \"npm run build:watch:server\" \"npm run build:watch:client\" \"nodemon ./build/server/index.js\"",
    "start:dev:client": "webpack-dev-server",
    "build:prod": "npm run build:server && npm run build:client",
    "start": "npm run build:prod && node ./build/server/index.js"
  },
  "author": "Elias Funes",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "html-webpack-plugin": "^3.0.6",
    "nodemon": "^1.17.1",
    "parallelshell": "^3.0.2",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  }
}
